{
  "name": "workspaces",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "workspaces",
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "packages/*",
        "packages/@slive/*"
      ]
    },
    "node_modules/@slive/app": {
      "resolved": "packages/@slive/app",
      "link": true
    },
    "node_modules/slive": {
      "resolved": "packages/slive",
      "link": true
    },
    "packages/@slive/app": {
      "version": "2.0.0",
      "license": "ISC"
    },
    "packages/slive": {
      "version": "3.0.0",
      "license": "ISC"
    }
  }
}
